@model IEnumerable<SUN2.Models.PersonAddGruppeModel>

@{
    ViewBag.Title = "Add";
}

<h2>Add</h2>



<p>
</p>
<table class="table">
    <tr>
        <th>
            <p>User ID</p>
        </th>
        <th>
            <p>E-Mail</p>
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.vorname)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.vorname)
            </td>
            <td>
                @Html.ActionLink("Add", "AddPerson", "MitgliederGruppe", new { gruppenid = item.gruppenid, id = item.id }, null)
            </td>
        </tr>
    }

</table>